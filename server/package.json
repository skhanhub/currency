{
  "name": "currency-analysis",
  "version": "1.0.0",
  "description": "A full stack application for analyzing currency based on historical data",
  "main": "./src/app.ts",
  "scripts": {
    "eslint": "eslint",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "coverage": "nyc mocha -r ts-node/register test/**/*.ts",
    "build": "tsc && node copyBuild.js",
    "build:watch": "tsc --w",
    "dev": "nodemon",
    "start": "tsc && node copyBuild.js && node ./dist/src/app.js"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html",
      "text"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shas38/currency-analysis.git"
  },
  "keywords": [
    "currency",
    "analysis"
  ],
  "author": "Shahriar Hasan Khan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shas38/currency-analysis/issues"
  },
  "homepage": "https://github.com/shas38/currency-analysis#readme",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/node": "^11.13.7",
    "body-parser": "^1.18.3",
    "copyfiles": "^2.1.0",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "fs-extra": "^7.0.1",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.4",
    "util": "^0.11.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/istanbul": "^0.4.30",
    "@types/mocha": "^5.2.6",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "source-map-support": "^0.5.12",
    "supertest": "^4.0.2"
  }
}
